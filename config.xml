    <splash src="/res/android/screen/splash-mdpi.png" density="mdpi"/>
  </platform>
  <preference name="WindowsStorePublisherName" value="My Name"/>
  <preference name="WindowsStoreIdentityName" value="33e7203f-8b60-a047-4b30-9e3566184bf2"/>
